<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.numberconversion">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NumberConversion">
        <activity android:name=".HexatoDec"></activity>
        <activity android:name=".DectoHexa" />
        <activity android:name=".OcttoDec" />
        <activity android:name=".DectoOct" />
        <activity android:name=".BinarytoDec" />
        <activity android:name=".DectoBinary" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>